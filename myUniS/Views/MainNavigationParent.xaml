             x:Class="myUniS.Views.MainNavigationParent"
             Title="MainNavigationParent"
             xmlns:views="clr-namespace:myUniS.Views"
	         xmlns:local="clr-namespace:myUniS"
             BackgroundColor="pink" Padding="20">

    <Tab>
